00038 {\cf18 void} SCD40_WRITE_COMMAND(uint16_t command)\{\par
00039     \par
00040     Command_address send_command;\par
00041     send_command.address = command;\par
00042     \par
00043     {\cf20 //Master starts performing a bus transaction}\par
00044     {\cf20 //when then the master address register is written}\par
00045     TWI0.MADDR = WRITE_OPCODE;\par
00046 \par
00047     {\cf20 //wait for data packet to be transmitted}\par
00048     {\cf19 while}(!(TWI0.MSTATUS & TWI_WIF_bm));\par
00049 \par
00050     TWI0.MDATA = send_command.address_msb;\par
00051 \par
00052     {\cf20 //wait for data packet to be transmitted}\par
00053     {\cf19 while}(!(TWI0.MSTATUS & TWI_WIF_bm));\par
00054 \par
00055     TWI0.MDATA = send_command.address_lsb;\par
00056 \par
00057     {\cf20 //wait for data packet to be transmitted}\par
00058     {\cf19 while}(!(TWI0.MSTATUS & TWI_WIF_bm));\par
00059 \par
