00088     {\cf19 while}(!(TWI0.MSTATUS & TWI_WIF_bm));\par
00089 \par
00090     TWI0.MDATA = send_command.address_lsb;\par
00091 \par
00092     {\cf20 //wait for data packet to be transmitted}\par
00093     {\cf19 while}(!(TWI0.MSTATUS & TWI_WIF_bm));\par
00094     \par
