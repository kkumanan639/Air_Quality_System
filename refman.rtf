00034     {\cf19 while}(!(TWI0.MSTATUS & TWI_WIF_bm));\par
00035     \par
00036     TWI0.MDATA = data;\par
00037     \par
00038     {\cf20 //wait for data packet to be transmitted}\par
00039     {\cf19 while}(!(TWI0.MSTATUS & TWI_WIF_bm));\par
00040     \par
00041     {\cf20 //complete transaction by issuing a stop condition}\par
00042     TWI0.MCTRLB = TWI_MCMD_gm;\par
00043     \par
00044 \}\par
