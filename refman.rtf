00011 {\cf20 /* Initialize USART */}\par
00012 {\cf18 void} UART_init(uint16_t baud, uint8_t data, {\cf18 unsigned} {\cf18 char} parity)\{\par
00013     {\cf20 //setting the Baud rate}\par
00014     USART3.BAUD = 1667;\par
00015     \par
00016     \par
00017     {\cf19 if}(parity == {\cf23 'E'})\{\par
00018         {\cf20 //Asynchronous mode, 0 - 8 bit data, no stop bit, 1 stop bit,  even parity}\par
00019         USART3.CTRLC =  USART_CMODE_ASYNCHRONOUS_gc | (data - 5) | USART_PMODE_EVEN_gc  |USART_SBMODE_1BIT_gc;\par
00020     \}\par
00021     {\cf19 else} {\cf19 if}(parity == {\cf23 'O'})\{\par
00022         {\cf20 //Asynchronous mode, 0 - 8 bit data, no stop bit, 1 stop bit,  odd parity}\par
00023         USART3.CTRLC =  USART_CMODE_ASYNCHRONOUS_gc | (data - 5) | USART_PMODE_ODD_gc  |USART_SBMODE_1BIT_gc;\par
00024     \}\par
00025     {\cf19 else}\{\par
00026         {\cf20 //Asynchronous mode, 0 - 8 bit data, no stop bit, 1 stop bit,  no parity}\par
00027         USART3.CTRLC =  USART_CMODE_ASYNCHRONOUS_gc | (data - 5) | USART_PMODE_DISABLED_gc  |USART_SBMODE_1BIT_gc;\par
00028     \}\par
00029     \par
00030     {\cf20 //configuring the TXD pin as an output}\par
00031     PORTB.DIR = PIN0_bm;\par
00032         \par
00033     {\cf20 //Normal mode and enable transmit}\par
00034     USART3.CTRLB =  USART_RXMODE_NORMAL_gc |  USART_TXEN_bm;\par
00035     \par
00036 \}\par
00037 \par
00038 {\cf18 void} UART_SendChar({\cf18 char} c)\{\par
00039     {\cf19 while}(!(USART3.STATUS & USART_DREIF_bm))\{\};\par
