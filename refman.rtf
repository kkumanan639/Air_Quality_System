00007 {\cf18 void} I2C0_MCP23017_init({\cf18 void})\{\par
00008     {\cf20 //configure PA2 (SDA)  PA3 (SCl) as inputs}\par
00009     PORTA.DIR = 0x00;\par
00010     \par
00011     {\cf20 //Master Baud rate set to 0}\par
00012     TWI0.MBAUD = 0;\par
00013     \par
00014     {\cf20 //Enable the TWI master}\par
00015     TWI0.MCTRLA = TWI_ENABLE_bm;\par
00016     \par
00017     {\cf20 //Forcing Bus state to Idle}\par
00018     TWI0.MSTATUS = TWI_BUSSTATE_IDLE_gc;\par
00019 \}\par
00020 \par
00021 \par
00022 {\cf18 void} MCP23017_I2C_write(uint8_t opcode, uint8_t slave_register, uint8_t data)\{\par
00023     \par
00024     {\cf20 //Master starts performing a bus transaction}\par
00025     {\cf20 //when then the master address register is written}\par
00026     TWI0.MADDR = opcode;\par
00027     \par
00028     {\cf20 //wait for data packet to be transmitted}\par
00029     {\cf19 while}(!(TWI0.MSTATUS & TWI_WIF_bm));\par
00030     \par
00031     TWI0.MDATA = slave_register;\par
00032     \par
